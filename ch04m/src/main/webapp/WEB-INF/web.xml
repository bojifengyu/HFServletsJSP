<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
	
	<display-name>Head First Servlets and JSP Chapter 4</display-name>
	<welcome-file-list>
		<welcome-file>/download.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>Chapter4 File Download</servlet-name>
		<servlet-class>com.jeremyallenshepherd.HFServletsJSP.ch04m.web.CodeReturn</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Chapter4 File Download</servlet-name>
		<url-pattern>/Code.do</url-pattern>
	</servlet-mapping>
</web-app>